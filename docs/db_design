account:
id			: primary key
nickname		: varchar(20)
auth_token		: varchar(255)
date_joined		: datetime
date_last_submission	: datetime
date_last_seen		: datetime
country_code		: varchar(3)
active			: bool

prefix:
id			: primary key
prefix			: inet6num
name_id			: foreign_key 'name.id'
date_seen_first		: datetime
date_seen_last		: datetime
season_first_seen	: foreign_key 'season.id'

observation:
id			: primary key
prefix_id		: foreign_key 'prefix.id'
season_id		: foreign_key 'season.id'
account_id  : foreign_key 'account.id'
location		: geo-location
point       : int

name:
id			: primary key
name_1			: foreign_key words.id
name_2			: foreign_key words.id
name_3			: foreign_key words.id
prefix_id		: foreign_key 'prefix.id'

score:
id			: primary key
account_id		: foreign_key 'account.id'
season_id		: foreign_key 'season.id'
value       : int

season:
id			: primary key
name			: varchar(100)
period_start		: datetime
period_duration		: int (days)

words:
id:  primary key
name : varchar(20)
